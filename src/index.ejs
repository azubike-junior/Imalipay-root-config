  <script src="https://cdn.jsdelivr.net/npm/import-map-overrides@2.2.0/dist/import-map-overrides.js"></script>
  <% if (isLocal) { %>
  <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/system.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/extras/amd.js"></script>
  <% } else { %>
  <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/system.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/systemjs@6.8.3/dist/extras/amd.min.js"></script>
  <% } %>

  <template id="single-spa-layout">
  <single-spa-router>
        <route path="/">
          <application name="@Imalipay/auth" props="basePaths" loader="moduleSpinner" error="moduleError"></application>
        </route>

        <route path="market-place">
          <div class="content">
          <application name="@Imalipay/marketPlace" loader="moduleSpinner" error="moduleError" props="basePaths"></application>
          </div>
        </route>
        <route path="device-management">
          <div class="content">
          <application name="@Imalipay/deviceManagement" loader="moduleSpinner" error="moduleError" props="basePaths"></application>
          </div>
        </route>

        <route path="users">
          <div class="content">
          <application name="@Imalipay/customers" loader="moduleSpinner" error="moduleError" props="basePaths"></application>
          </div>
        </route>

        <route path="payments">
          <div class="content">
          <application name="@Imalipay/payments" loader="moduleSpinner" error="moduleError" props="basePaths"></application>
          </div>
        </route>

        <route path="loans">
          <!-- <div class="content"> -->
          <application name="@Imalipay/loans" loader="moduleSpinner" error="moduleError" props="basePaths"></application>
          <!-- </div> -->
        </route>
      
  </div>

  </main>
</single-spa-router>
</template>

</head>
<body> 
  <noscript>
    You need to enable JavaScript to run this app for real.
  </noscript>
  <script>
    System.import('@Imalipay/root-config');
  </script>
  <import-map-overrides-full show-when-local-storage="devtools" dev-libs></import-map-overrides-full>
</body>
